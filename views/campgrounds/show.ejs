<% layout('/layouts/boilerplate.ejs')%>
<ul>
  <li><%=campground.title%></li>
  <li><%=campground.description%></li>
  <li><%=campground.price%></li>
  <li><%=campground.location%></li>
</ul>
<form method="GET" action="/campgrounds/<%=campground._id%>/edit">
  <button type="submit">Edit Campground</button>
</form>
<form method="POST" action="/campgrounds/<%=campground._id%>?_method=DELETE">
  <button>Delete</button>
</form>
<a href="/campgrounds">Back to campgrounds</a>
